    <script>


        const yearSelect = document.getElementById('exampleFormControlSelect1');
        const dataTableBody = document.querySelector('#data-table tbody');
        const currentYear = new Date().getFullYear();

        // Populate the year dropdown list
        for (let year = currentYear; year >= 1999; year--) {
            const option = document.createElement('option');
            option.value = year;
            option.text = year;
            yearSelect.add(option);
        }

        // Add event listener for when the user selects a year
        yearSelect.addEventListener('change', () => {
            const selectedYear = yearSelect.value;

            // Make a request to the backend to get data for the selected year
            fetch(`/manager/get_student_details?startyear=${selectedYear}&course={{course}}`)
                .then(response => response.json())
                .then(data => {
                    let tableData = document.getElementById('table-data');
                    let noDataRow = document.getElementById('no-data-row');
                    // Remove existing rows from the table
                    while (tableData.firstChild) {
                        tableData.removeChild(tableData.firstChild);
                    }
                    if (data === null) {
                        noDataRow.style.display = 'table-row';
                    } else {
                        data.forEach(item => {
                            let row = document.createElement('tr');
                            let nameCell = document.createElement('td');
                            let DOBCell = document.createElement('td');
                            let emailCell = document.createElement('td');
                            let genderCell = document.createElement('td');
                            let mobileCell = document.createElement('td');
                            let courseCell = document.createElement('td');
                            let addressCell = document.createElement('td');

                            nameCell.innerText = item.firstName + item.lastName;
                            DOBCell.innerText = item.DOB;
                            emailCell.innerText = item.email;
                            mobileCell.innerText = item.mobile
                            courseCell.innerText = item.course
                            addressCell.innerText = item.Address
                            genderCell.innerText = item.gender


                            row.appendChild(nameCell);
                            row.appendChild(genderCell);
                            row.appendChild(DOBCell);
                            row.appendChild(courseCell);
                            row.appendChild(mobileCell)
                            row.appendChild(emailCell);

                            row.appendChild(addressCell);

                            

                            tableData.appendChild(row);

                        });
                    }
                });
        })


    </script>